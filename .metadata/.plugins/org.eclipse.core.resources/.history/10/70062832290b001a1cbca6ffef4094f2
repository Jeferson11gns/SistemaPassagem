package sistema.model;

import java.util.Calendar;

public class Usuario {
	
	private String nome;
	private String cpf;
	private String email;
	private Calendar dataAdmissa;
	
	public Usuario() {}
	
	public Usuario(String nome, String cpf, String email, Calendar dataAdmissa) {
		super();
		this.nome = nome;
		this.cpf = cpf;
		this.email = email;
		this.dataAdmissa = dataAdmissa;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public String getCpf() {
		return cpf;
	}

	public void setCpf(String cpf) {
		this.cpf = cpf;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public Calendar getDataAdmissa() {
		return dataAdmissa;
	}

	public void setDataAdmissa(Calendar dataAdmissa) {
		this.dataAdmissa = dataAdmissa;
	}

	@Override
	public String toString() {
		return "Usuario [nome=" + nome + ", cpf=" + cpf + ", email=" + email + ", dataAdmissa=" + dataAdmissa + "]";
	}
	
	s
}
