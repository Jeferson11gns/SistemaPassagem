package sistema.model;

import java.util.Calendar;

public class Usuario {
	
	private String nome;
	private String cpf;
	private String email;
	private Calendar dataAdmissa;
	
	public Usuario() {}
	
	public Usuario(String nome, String cpf, String email, Calendar dataAdmissa) {
		super();
		this.nome = nome;
		this.cpf = cpf;
		this.email = email;
		this.dataAdmissa = dataAdmissa;
	}
	
}
